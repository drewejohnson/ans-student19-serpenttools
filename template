 1001.06c 2
 8016.06c 1

therm lwter lwe7.12t

mat fuel -10.29748  burn 1 vol 14.3350752
 O-16.06c 4.58500E-02
 O-17.06c 1.74660E-05
U-234.06c 5.79870E-06
U-235.06c 7.21750E-04
U-238.06c 2.22530E-02

% --- "Zircaloy-4" [PNNL-15870, Rev. 1]

mat zircaloy -6.56000E+00
 8016.09c  -1.19600E-03
24000.09c  -9.97000E-04
26000.09c  -1.99400E-03
40000.09c  -9.81858E-01
50000.09c  -1.39550E-02

surf 10 cylz 0.0 0.0 {fuelr}
surf 11 cylz 0.0 0.0 {cladr}
surf 12 sqc  0.0 0.0 {mpitch}

cell 1 0 fuel -10
cell 2 0 zircaloy 10 -11
cell 3 0 water 11 -12
cell 4 0 outside 12

set bc 2
set pop 1000 100 100
set acelib "sss_endfb7u.xsdata"
